{
  "name": "anikku-app.github.io",
  "type": "module",
  "version": "3.0.0",
  "private": true,
  "packageManager": "pnpm@9.15.9",
  "description": "(Stolen) Official website for the Anikku app.",
  "license": "MPL-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anikku-app/anikku-app.github.io.git"
  },
  "bugs": {
    "url": "https://github.com/anikku-app/anikku-app.github.io/issues"
  },
  "engines": {
    "node": ">=20",
    "pnpm": ">=8"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "test": "pnpm lint && pnpm build",
    "dev": "vitepress dev src",
    "build": "vitepress build src",
    "preview": "vitepress preview src",
    "lint": "pnpm lint:es && pnpm lint:mdl && pnpm lint:style",
    "lint:fix": "pnpm lint:es:fix && pnpm lint:style:fix",
    "lint:es": "eslint . ",
    "lint:es:fix": "eslint . --fix",
    "lint:mdl": "markdownlint \"**/*.md\" \".github/**/*.md\" --enable sentences-per-line --disable MD025 MD033",
    "lint:style": "stylelint '**/*.{styl,vue}' 'src/.vitepress/**/*.{styl,vue}'",
    "lint:style:fix": "stylelint --fix '**/*.{styl,vue}' 'src/.vitepress/**/*.{styl,vue}'"
  },
  "dependencies": {
    "@iconify-prerendered/vue-mdi": "0.28.1737398331",
    "@octokit/rest": "21.1.1",
    "@octokit/types": "13.10.0",
    "@tanstack/vue-query": "5.92.8",
    "@vueuse/core": "11.3.0",
    "axios": "1.13.5",
    "element-plus": "2.13.1",
    "lodash.groupby": "4.6.0",
    "markdown-it": "14.1.1",
    "markdown-it-shortcode-tag": "1.1.0",
    "moment": "2.30.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.26.0",
    "@eslint/eslintrc": "^3.1.0",
    "@mdit/plugin-attrs": "0.24.1",
    "@mdit/plugin-figure": "0.22.2",
    "@mdit/plugin-img-lazyload": "0.22.1",
    "@mdit/plugin-img-mark": "0.22.2",
    "@mdit/plugin-img-size": "0.22.4",
    "@mdit/plugin-include": "0.22.3",
    "@resvg/resvg-js": "2.6.2",
    "@types/gtag.js": "0.0.20",
    "@types/lodash.groupby": "4.6.9",
    "@types/markdown-it": "14.1.2",
    "@types/node": "24.11.0",
    "@typescript-eslint/eslint-plugin": "8.53.0",
    "@typescript-eslint/parser": "8.53.0",
    "eslint": "9.39.2",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-vue": "9.33.0",
    "feed": "4.2.2",
    "lint-staged": "15.5.2",
    "markdownlint": "0.40.0",
    "markdownlint-cli": "0.47.0",
    "sentences-per-line": "0.5.0",
    "stylelint": "16.26.1",
    "stylelint-stylus": "1.0.0",
    "stylus": "0.64.0",
    "unplugin-element-plus": "0.11.2",
    "vite-plugin-eslint": "1.8.1",
    "vitepress": "1.6.4",
    "vitepress-plugin-tabs": "0.7.3",
    "vue": "3.5.26",
    "vue-eslint-parser": "9.4.3",
    "x-satori": "0.4.0"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*.{styl,vue}": "stylelint --fix",
    "*.{html,json}": "prettier --write"
  }
}
